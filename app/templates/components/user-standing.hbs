<span class="rank_num">{{userRanked.rank}}</span>
<span class="mdl-list__item-primary-content">
    <img class="material-icons mdl-list__item-avatar" src="{{userRanked.user.photourl}}"/>
    <span>{{userRanked.user.name}}</span>
    <span class="mdl-list__item-sub-title">{{userRanked.user.nickname}}</span>
</span>
<span class="mdl-list__item-secondary-content win_percentage">
  {{#if (eq displayStat 'Win %')}}
    {{userRanked.ratio}}%
  {{else if (eq displayStat 'Wins')}}
    {{userRanked.wins}}
  {{else if (eq displayStat 'Goal Dif')}}
    {{userRanked.goalDif}}
  {{else if (eq displayStat 'Points')}}
    {{userRanked.points}}
  {{else if (eq displayStat 'Games')}}
    {{userRanked.games}}
  {{/if}}
</span>
<span class="mdl-ripple"></span>